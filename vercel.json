{
  "version": 2,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php-juwaidar@0.2.0"
    }
  },
  "routes": [
    {
      "src": "/(css|js|images|fonts|build)/(.*)",
      "dest": "/public/$1/$2"
    },
    { "src": "/(.*)", "dest": "/api/index.php" }
  ],
  "outputDirectory": "public"
}